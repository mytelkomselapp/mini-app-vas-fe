  "scripts": {
    "set-appid": "node setAppId.js",
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "clean-install": "rm -rf node_modules dist && npm install --legacy-peer-deps",
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "cross-env NODE_ENV=development yarn run set-appid && yarn run clean-install && NODE_ENV=development taro build --type qq",
    "build:qq-production": "cross-env NODE_ENV=production yarn run set-appid && yarn run clean-install && NODE_ENV=production taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:harmony-hybrid": "taro build --type harmony-hybrid",
    "dev:weapp": "NODE_ENV=development npm run build:weapp -- --watch",
    "dev:swan": "NODE_ENV=development npm run build:swan -- --watch",
    "dev:alipay": "NODE_ENV=development npm run build:alipay -- --watch",
    "dev:tt": "NODE_ENV=development npm run build:tt -- --watch",
    "dev:h5": "NODE_ENV=development npm run build:h5 -- --watch",
    "dev:rn": "NODE_ENV=development npm run build:rn -- --watch",
    "dev:qq": "cross-env NODE_ENV=development yarn run set-appid && npm run build:qq -- --watch",
    "dev:qq-production": "cross-env NODE_ENV=production yarn run set-appid && npm run build:qq-production -- --watch",
    "dev:jd": "NODE_ENV=development npm run build:jd -- --watch",
    "dev:harmony-hybrid": "NODE_ENV=development npm run build:harmony-hybrid -- --watch",
    "postinstall": "weapp-tw patch"
  },
  "browserslist": [
    "defaults and fully supports es6-module",
    "maintained node versions"
  ],
